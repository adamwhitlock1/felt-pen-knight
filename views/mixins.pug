mixin renderFrame(frame)
	div.frame.content
		a(href = '/frames/' + frame.id)
			img.frame__info(src = '/static/images/' + frame.url)
		p.frame__info
			a.frame__info__user(href = '/users/' + frame.author.id) #{frame.author.name}
			i  | #{frame.created} |
			if frame.children
				b Children: #{frame.children.length} 
